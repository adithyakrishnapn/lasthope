<section class="lsfnpanel">
    <div class="container">
        <div class="row">
            <div class="form">
                <form action="/foundGov" method="post" enctype="multipart/form-data">
                    <div id="center">
                        <h2 style="color: white;">What did you found ?</h2>
                        <label for="category">Select category:</label>
                        <select name="category" id="category">
                            <option value="Aadhar Card">Aadhar Card</option>
                            <option value="Pan Card">Pan Card</option>
                            <option value="Ration Card">Ration Card</option>
                            <option value="Driving Liscence">Driving License</option>
                            <option value="Other">Other</option>
                        </select>
                        <input type="text" placeholder="Specify Category" id="newcat">
                        <input type="text" name="check" value="found" hidden>
                        <button type="button" onclick="details(event)">Next</button>
                    </div>
                    <div id="detail">
                        <h2 style="color: white;" id="headingdetails">Fill the details</h2>
                        <label for="">Image</label><br>
                        <input type="file" name="Image"><br>
                        <label id="numberlabel">Aadhar Number : </label><br>
                        <input type="number" name="number" id="aadharNumber" placeholder="" required><br>

                        <label id="namelabel">Aadhar Holder Name : </label><br>
                        <input type="text" name="name" id="aadharName" placeholder="" required><br>

                        <label>Where did you find it : </label><br>
                        <input type="text" name="findPlace" placeholder="Place where you find it ?" required><br>

                        <label>Tell something ? : </label><br>
                        <textarea id="w3review" name="review" rows="4" cols="30" placeholder="Specify....."
                            required></textarea><br>
                        <br><button type="submit">Post</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>
<script src="/javascripts/foundandwanted.js"></script>